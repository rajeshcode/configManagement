<?xml version="1.0"?> 

<configuration> 

<!-- system limit, across all queues --> 

<property> 
  <name>mapred.capacity-scheduler.maximum-system-jobs</name> 
  <value>3000</value> 
<description>Maximum number of jobs in the system which can be initialized, 
concurrently, by the Capacity Scheduler. 
</description> 
</property> 

